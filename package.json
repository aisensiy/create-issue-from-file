    "@actions/core": "1.4.0",
    "@actions/github": "5.0.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "15.12.4",
    "@typescript-eslint/parser": "4.28.0",
    "@vercel/ncc": "0.28.6",
    "eslint": "7.29.0",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-jest": "24.3.6",
    "jest": "27.0.5",
    "jest-circus": "27.0.5",
    "js-yaml": "4.1.0",
    "prettier": "2.3.1",
    "ts-jest": "27.0.3",
    "typescript": "4.3.4"
  }
